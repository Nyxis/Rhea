{% if tasks is not empty %}
<div class="box next_tasks">
    <div class="tab-header"><i class="icon-calendar"></i> {{ 'next_tasks_header'|trans }}</div>
    <ul class="">
        {% for task in tasks %}
            {% include 'ExtiaDashboardBundle:Task:task_element.html.twig' with { 'task': task } only %}
        {% endfor %}
    </ul>
</div>
{% endif %}
